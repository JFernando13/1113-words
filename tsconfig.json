{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "~/components/*": ["src/components/*"],
      "~/layouts/*": ["src/layouts/*"],
      "~/utils/*": ["src/utils/*"],
      "~/icons/*": ["src/components/icons/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
